	<queryString language="SQL">
		<![CDATA[SELECT 
		count(a.codCancion) cantidadCancion,
		b.nombreCancion,
		c.descGenero
	FROM compraDetalle a
	INNER JOIN canciones b
		on a.codCancion = b.codCancion
